        <name>Populate Contract Field</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

	var companyArray = g_form.getValue('please_select_icon_company_s_that_access_should_be_added');

	var ga = new GlideAjax('iCONUtils');
	ga.addParam('sysparm_name', 'splitCompany');
	ga.addParam('sysparm_company', companyArray);
	ga.getXML(getCompany);
 
	function getCompany(response) {
	  var answer = response.responseXML.documentElement.getAttribute("answer");
	  g_form.setValue('contract_range', answer);
	  }

   
	//g_form.addInfoMessage(g_form.getValue(companyArray));
  // g_form.setValue('contract_range', 'hi');
	
}]]></script>
