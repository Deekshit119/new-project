        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	//gs.addInfoMessage('current.variables.generic_contact_type='+current.variables.generic_contact_type);
//gs.info('jesse 1');
	var grRITM = new GlideRecord('sc_req_item');
	grRITM.addQuery('request', current.sys_id);
	grRITM.query();
	//gs.info('jesse 2');
	if(grRITM.next()){
		//gs.info('jesse 3');
		if(grRITM.variables.please_select_the_user_account_you_would_like_to_modify){ // will only run on icon request item
			//gs.info('jesse 4');
			current.setValue('short_description', 'iCON Account Request');
			current.setValue('requested_for', grRITM.variables.please_select_the_user_account_you_would_like_to_modify);
			current.update();
			//gs.info('jesse 5');
		}
	}
})(current, previous);]]></script>
