        <source_script><![CDATA[answer = (function transformEntry(source) {

	var str = source.u_user_name;
	var	username  = str.substring(str.indexOf(" (") -7 , str.indexOf(" ("));
	
	var user = new GlideRecord('sys_user');
	user.addEncodedQuery("user_nameENDSWITH"+username);
	user.query();
	if(user.next()){
		return user.sys_id;
	}
	
	
})(source);]]></source_script>
