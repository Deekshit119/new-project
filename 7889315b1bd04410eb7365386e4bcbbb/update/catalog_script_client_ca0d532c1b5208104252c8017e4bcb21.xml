        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
	if (isLoading || newValue == '') {
		return;
	}
	
	//Type appropriate comment here, and begin script below
	var gr = new GlideRecord('cmn_location');
	gr.addQuery('sys_id',newValue);
	gr.query(fillFields);
	//alert('newvalue ' + newValue);
	
	function fillFields(gr)
	{
		if (gr.next())
			{
			//alert('in function');
			g_form.setValue('location_address',gr.street);
			g_form.setValue('location_city',gr.city);
			g_form.setValue('location_county',gr.state);
			g_form.setValue('location_post_code',gr.zip);				
			
			
		}
	}
	
}]]></script>
