        <value>// Set the variable 'answer' to true or false to indicate if the condition has been met or not.&#13;
var complete;&#13;
var incomplete;&#13;
var open;&#13;
var ritm = new GlideRecord('sc_req_item');&#13;
ritm.addQuery('request',current.sys_id);&#13;
ritm.query();&#13;
while (ritm.next())&#13;
	{&#13;
		if (ritm.getValue('state') == 3) // closed complete&#13;
			complete = true;&#13;
		else if (ritm.getValue('state') == 4) // closed incomplete&#13;
			incomplete = true;&#13;
		else if (ritm.getValue('state') == 7) // closed incomplete&#13;
			incomplete = true;&#13;
		else  // states like Pending, awaitimf approval, open, work in progress&#13;
			open = true;&#13;
	}&#13;
if (open)&#13;
	{&#13;
		answer = false;		&#13;
	}&#13;
else if (incomplete)&#13;
	{&#13;
		answer = true;&#13;
		workflow.scratchpad.incomplete = true;&#13;
	}&#13;
else if (complete)&#13;
	{&#13;
		answer = true;&#13;
		workflow.scratchpad.complete = true;&#13;
		&#13;
	}&#13;
&#13;
&#13;
&#13;
</value>
