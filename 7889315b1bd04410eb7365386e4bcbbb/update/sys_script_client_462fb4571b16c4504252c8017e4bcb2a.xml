        <order/>
        <script><![CDATA[function onLoad() {
	
	if(!g_user.hasRole('change_manager') && g_form.getValue('state') == -2){

	var fields = g_form.getEditableFields();
	for (var x = 0; x < fields.length; x++) {
	
	var myScope = "x_sksw_uk_itsm";
	var field = fields[x];
	var fElement = g_form.getGlideUIElement(field);
	var originalScope = fElement.getScope() + "";
	
	fElement.scope = myScope;
	g_form.setReadOnly(field, true);
	fElement.scope = originalScope;
	
}
	}
	
	
}]]></script>
