        <script><![CDATA[function moveToScheduled(){
	var ga = new GlideAjax("global.ChangeRequestStateHandlerAjax");
	ga.addParam("sysparm_name", "getStateValue");
	ga.addParam("sysparm_state_name", "scheduled");
	ga.getXMLAnswer(function(stateValue) {
		g_form.setValue("state", stateValue);
		gsftSubmit(null, g_form.getFormElement(), "state_model_move_to_scheduled");
	});
}

